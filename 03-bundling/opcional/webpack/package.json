    "@babel/preset-typescript": "^7.23.3",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@types/typescript": "^2.0.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
